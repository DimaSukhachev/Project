        <Button Content="Гимн России" HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Margin="10,75,0,0" Style="{StaticResource PalmGod2}" Click="Button_Click_1"/>
        <Frame x:Name="PageViewer" Margin="182,0,124,0"/>
        <Grid HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="486" Margin="182,0,0,0"/>

        <Grid HorizontalAlignment="Left" Height="420" Margin="668,0,0,0" VerticalAlignment="Top" Width="124">

            <Grid Background="Aqua" Grid.RowSpan="2"></Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="103*"/>
                <RowDefinition Height="107*"/>
            </Grid.RowDefinitions>
        </Grid>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="668,0,0,0" Height="116" Width="124"/>









    </Grid>
</Window>
