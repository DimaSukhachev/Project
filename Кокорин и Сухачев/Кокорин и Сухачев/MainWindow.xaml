            <Button Content="Курс Рубля" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" Width="162" Style="{StaticResource PalmGod3}" Click="Button_Click_2"/>
        </Grid>
        <Button HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Margin="10,10,0,0" Style="{StaticResource PalmGod}" Click="Button_Click_3" Height="26" Content="Новости&#xD;&#xA;"/>
